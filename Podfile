# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Messenger' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Messenger

# Firebase
pod 'Firebase/Core'
pod 'Firebase/Auth'
pod 'Firebase/Database'
pod 'Firebase/Storage'

# Facebook Sign in 
pod 'FBSDKLoginKit'

# Google Sign in 
pod 'GoogleSignIn'

pod 'MessageKit'
pod 'RealmSwift'
pod 'JGProgressHUD'
pod 'SDWebImage'

end
